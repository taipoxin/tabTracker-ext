{
  "manifest_version": 2,

  "name": "Tab Tracker",
  "description": "tracker",
  "version": "1.0",

  "browser_action": {
      "default_icon": "./src/icon.png",
      "default_popup": "./src/popup.html"
  },

  "background": {
    "page": "./src/background.html",
    "persistent": false
  },

  "permissions": [
      "tabs"
  ]
}