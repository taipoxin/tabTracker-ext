{
  "manifest_version": 2,

  "name": "Tab Tracker",
  "description": "tracker",
  "version": "1.0",

  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": [
      "tabs"
  ]
}